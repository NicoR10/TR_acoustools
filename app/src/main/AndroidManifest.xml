<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tracoustools">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Sala_act"
            android:label="@string/SALA"
            android:theme="@style/SalaTheme" />
        <activity
            android:name=".Superficies_act"
            android:label="@string/SUPERFICIES"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NuevaSup_act"
            android:label="@string/nueva_sup"
            android:theme="@style/nuevasupTheme">

        </activity>
    </application>

</manifest>